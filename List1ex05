 double n1;
    double n2;
    double n3;
    double n4;
    double media;

    Console.WriteLine("Digite seu primeiro numero");
    n1 = double.Parse(Console.ReadLine());
     Console.WriteLine("Digite seu segundo numero");
    n2 = double.Parse(Console.ReadLine());
     Console.WriteLine("Digite seu terceiro numero");
    n3 = double.Parse(Console.ReadLine());
     Console.WriteLine("Digite seu quarto numero");
    n4 = double.Parse(Console.ReadLine());
    media = (n1+n2+n3+n4)/4;
    Console.WriteLine("Resultado da media");
    Console.WriteLine(media);
